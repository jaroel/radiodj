{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "xo",
        "xo/browser"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", 2],
        "prefer-template": "warn"
    }
}
